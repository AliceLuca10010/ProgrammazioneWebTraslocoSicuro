        <div class="row">
            <div class="col">
                <h3 class="p-4 text-center"> Servizi Aggiuntivi Scelti</h3>

            </div>
        </div>

        <div class="row">

            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th scope="col">Servizi Aggiuntivi</th>
                        <th scope="col">Scelto</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Imballaggio</td>
                        <td>
                            <%if (DatiPrenotazione.imballaggio==="true") {%>SI
                                <%}else{%>NO
                                    <%}%>
                        </td>
                    </tr>
                    <tr>
                        <td>Smontaggio/Rimontaggio</td>
                        <td>
                            <%if (DatiPrenotazione.smontaggioRiassemblaggio==="true") {%>SI
                                <%}else{%>NO
                                    <%}%>
                        </td>
                    </tr>
                    <tr>
                        <td>Deposito Merci </td>
                        <td>
                            <%if (DatiPrenotazione.depositoMerci==="true") {%>SI
                                <%}else{%>NO
                                    <%}%>
                        </td>
                    </tr>

                </tbody>
            </table>


        </div>

        <div class="row">
            <div class="col-6 p-5 text-left">
                <form action="/cancellaPrenotazione" method="POST">
                    <button class="btn btn-danger"><%if(!preventivoConfermato){%>Annulla
                            <%}else{%>Cancella Prenotazione
                                <%}%></button>
                </form>
            </div>
            <div class="col-6 p-5 text-right">
                <h5>Costo totale:
                    <h1>
                        <%=costoTotale%> €
                    </h1>

                </h5>
                <p>Il costo è comprensivo delle maggiorazioni dovute alla difficoltà del trasloco e delle distanze. </p>
            </div>
        </div>
        <% if (!preventivoConfermato) {%>
            <form action="/confermaPreventivo" method="POST">
                <div class="row">
                    <div class="col p-3 text-center">
                        <button type="submit" class="btn btn-info">Conferma</button>
                    </div>
                </div>
            </form>
            <% } %>
                <% } else { %>
                    <div class="container p-5">
                        <div class="row">
                            <div class="col text-center">
                                <h3 class="text-center">Non hai ancora effettuato nessuna prenotazione</h3>
                            </div>
                        </div>
                    </div>
                    <% } %>
    </div>

